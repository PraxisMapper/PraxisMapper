  "reprocessJson": false, //false: only read PBF files. true: load a JSON file and follow normal processing rules on it, then re-do output. Only converts items to center points at this time.
  "singleStyle": "", //if not blank, pull out one named style from the default styleSet and use that in place of the default styleset this run.
  "MapTilesEngine": "ImageSharp"
}