{
  "DbMode": "MariaDB",
  "DbConnectionString": "server=localhost;database=praxis;user=root;password=root;",
  "ForceSeparateFiles": false,
  "PbfFolder": "C:\\Praxis\\",
  "JsonMapDataFolder": "C:\\Praxis\\",
  "Solar2dExportFolder": "",
  "UseHighAccuracy": true,
  "OnlyTaggedAreas": false,
  "ForceTagParserDefaults": false,
  "UseMariaDBInFile": false,
  "UseOneRelationID": 0,
  "LogLevel": 1,
  "KeepElementsInMemory": false,
  "TagParserStyleSet": "mapTiles",
  "mapTileScaleFactor": 2,
  "slippyTileSize": 512, //is in db settings already.
  "placesAreaBuffer": 0.000125, //1 Cell10
