  <ItemGroup>
    <Folder Include="Interfaces\" />
    <Folder Include="CustomOsmSharp\" />
  </ItemGroup>

  <ItemGroup>
    <None Update="CustomOsmSharp\OsmSharp.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="CustomOsmSharp\OsmSharp.Geo.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
